{
    "name": "Sto Theme",
    "type": "dark",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "function": {
            "foreground": "#ccc",
            "italic": true
        },
        "variable": {
            "foreground": "#cccccc",
            "italic": true
        },
        "interface": {
            "foreground": "#FF68B8",
            "italic": true
        },
        "type": {
            "foreground": "#FF68B8",
            "italic": true
        },
        // This colours both object definiton properties, as well as references. If the property does not exist, it will be white\.
        "property": "#FF68B8"
    },
    "colors": {
        // menu
        "menu.selectionForeground": "#ccc",
        "menubar.selectionBackground": "#131415",
        // activityBar
        "activityBar.background": "#131415",
        "activityBar.border": "#131415",
        "activityBar.dropBackground": "#131415",
        "activityBar.foreground": "#ccc",
        "activityBarBadge.background": "#42A1FF",
        "activityBarBadge.foreground": "#131415",
        // badge
        "badge.background": "#42A1FF",
        "badge.foreground": "#131415",
        // button
        "button.background": "#42A1FF",
        "button.foreground": "#131415",
        "button.hoverBackground": "#42A1FF90",
        // contrast
        // "contrastActiveBorder": null,
        "contrastBorder": "#ffffff00",
        // debug
        "debugExceptionWidget.background": "#131415",
        "debugExceptionWidget.border": "#ccc",
        "debugToolBar.background": "#131415",
        // description
        "descriptionForeground": "#ccc",
        // diff
        "diffEditor.insertedTextBackground": "#4ad96233",
        "diffEditor.insertedTextBorder": "#4ad96255",
        "diffEditor.removedTextBackground": "#ee3a4333",
        "diffEditor.removedTextBorder": "#ee3a4355",
        // dropdown
        "dropdown.background": "#131415",
        "dropdown.border": "#131415",
        "dropdown.foreground": "#fff",
        // editor
        // This is the main background color
        "editor.background": "#131415",
        // this is the main text colour
        "editor.foreground": "#ccc",
        // Okay this part is confusing as heck!
        // Currently found item
        "editor.findMatchBackground": "#42A1FF66",
        // Other Found Items int the document
        "editor.findMatchHighlightBackground": "#4ad96266",
        // WTF is this one for? I don't know
        "editor.findRangeHighlightBackground": "#243E51",
        // When you hover over something and a popup shows, this highlights that thing
        "editor.hoverHighlightBackground": "#42A1FF33",
        // when you have something selected, but have lost focus on the editor
        "editor.inactiveSelectionBackground": "#00000025",
        // current line styles
        "editor.lineHighlightBackground": "#ffffff05",
        "editor.lineHighlightBorder": "#ffffff05",
        "editor.rangeHighlightBackground": "#ffffff05",
        // selected Text colours
        // This is the standard Select colour
        "editor.selectionBackground": "#000",
        // This is the colour of the other matching elements
        "editor.selectionHighlightBackground": "#00000080",
        // if you tab away you can colour it differently, but ill leave this one out
        // "editor.inactiveSelectionBackground": "",
        // Word Highlights! This happens when you move your cursor inside a variable
        // Strong is the one where your cursor currently is
        "editor.wordHighlightStrongBackground": "#ffffff21",
        // and this one is the rest of them
        "editor.wordHighlightBackground": "#ffffff21",
        "editorBracketMatch.background": "#131415",
        "editorBracketMatch.border": "#42A1FF80",
        "editorCodeLens.foreground": "#ccc",
        "editorCursor.foreground": "#42A1FF",
        "editorError.border": "#131415",
        "editorError.foreground": "#cb2d25",
        // gutter
        "editorGutter.background": "#13141566",
        "editorGutter.addedBackground": "#3C9F4A",
        "editorGutter.deletedBackground": "#cb2d25",
        "editorGutter.modifiedBackground": "#26506D",
        // editorGroup
        "editorGroup.background": "#cb2d25",
        "editorGroup.border": "#131415",
        "editorGroup.dropBackground": "#13141599",
        // editorGroupHeader
        "editorGroupHeader.noTabsBackground": "#131415",
        "editorGroupHeader.tabsBackground": "#131415",
        "editorGroupHeader.tabsBorder": "#131415",
        // editorHoverWidget
        "editorHoverWidget.background": "#131415",
        "editorHoverWidget.border": "#131415",
        "editorIndentGuide.background": "#3B5364",
        "editorLineNumber.foreground": "#ccc",
        "editorLink.activeForeground": "#ccc",
        // editorMarkerNavigation
        "editorMarkerNavigation.background": "#3B536433",
        "editorMarkerNavigationError.background": "#cb2d25",
        "editorMarkerNavigationWarning.background": "#42A1FF",
        // ruler
        "editorOverviewRuler.border": "#131415",
        "editorOverviewRuler.commonContentForeground": "#42A1FF55",
        "editorOverviewRuler.currentContentForeground": "#cb2d2555",
        "editorOverviewRuler.incomingContentForeground": "#4ad96255",
        "editorRuler.foreground": "#1F4662",
        // editorSuggestWidget
        "editorSuggestWidget.background": "#131415",
        "editorSuggestWidget.border": "#131415",
        "editorSuggestWidget.foreground": "#ccc",
        "editorSuggestWidget.highlightForeground": "#42A1FF",
        "editorSuggestWidget.selectedBackground": "#193549",
        // editorWarning
        "editorWarning.border": "#ffffff00",
        "editorWarning.foreground": "#42A1FF",
        "editorWhitespace.foreground": "#ffffff52",
        "editorWidget.background": "#131415",
        "editorWidget.border": "#131415",
        "errorForeground": "#cb2d25",
        // extensionButton
        "extensionButton.prominentBackground": "#42A1FF",
        "extensionButton.prominentForeground": "#fff",
        "extensionButton.prominentHoverBackground": "#0088ff",
        "focusBorder": "#131415",
        "foreground": "#ccc",
        // input
        "input.background": "#ffffff10",
        "input.border": "#131415",
        "input.foreground": "#FFF",
        "input.placeholderForeground": "#ccc",
        "inputOption.activeBorder": "#8dffff",
        "inputValidation.errorBackground": "#131415",
        "inputValidation.errorBorder": "#42A1FF",
        "inputValidation.infoBackground": "#131415",
        "inputValidation.infoBorder": "#131415",
        "inputValidation.warningBackground": "#131415",
        "inputValidation.warningBorder": "#42A1FF",
        // list
        "list.activeSelectionBackground": "#000",
        "list.activeSelectionForeground": "#fff",
        "list.dropBackground": "#131415",
        "list.focusBackground": "#131415",
        "list.focusForeground": "#fff",
        "list.highlightForeground": "#42A1FF",
        "list.hoverBackground": "#000",
        "list.hoverForeground": "#fff",
        "list.inactiveSelectionBackground": "#000",
        "list.inactiveSelectionForeground": "#fff",
        // menu
        "menu.background": "#131415",
        // merge
        "merge.border": "#ffffff00",
        "merge.commonContentBackground": "#cc8f00",
        "merge.commonHeaderBackground": "#cc8f00",
        "merge.currentContentBackground": "#53a33b",
        "merge.currentHeaderBackground": "#53a33b",
        "merge.incomingContentBackground": "#3480cc",
        "merge.incomingHeaderBackground": "#3480cc",
        // notification colors - The colors below only apply for VS Code versions 1.21 and higher.
        "notificationCenter.border": "#42A1FF",
        "notificationCenterHeader.foreground": "#ccc",
        "notificationCenterHeader.background": "#131415",
        "notificationToast.border": "#42A1FF",
        "notifications.foreground": "#ccc",
        "notifications.background": "#131415",
        "notifications.border": "#42A1FF",
        "notificationLink.foreground": "#42A1FF",
        // panel
        "panel.background": "#131415",
        "panel.border": "#42A1FF",
        "panelTitle.activeBorder": "#42A1FF",
        "panelTitle.activeForeground": "#42A1FF",
        "panelTitle.inactiveForeground": "#ccc",
        // "peekView
        "peekView.border": "#42A1FF",
        "peekViewEditor.background": "#131415",
        "peekViewEditor.matchHighlightBackground": "#42A1FF00",
        "peekViewEditorGutter.background": "#131415",
        "peekViewResult.background": "#131415",
        "peekViewResult.fileForeground": "#ccc",
        "peekViewResult.lineForeground": "#fff",
        "peekViewResult.matchHighlightBackground": "#131415",
        "peekViewResult.selectionBackground": "#131415",
        "peekViewResult.selectionForeground": "#fff",
        "peekViewTitle.background": "#131415",
        "peekViewTitleDescription.foreground": "#ccc",
        "peekViewTitleLabel.foreground": "#42A1FF",
        // picker
        "pickerGroup.border": "#131415",
        "pickerGroup.foreground": "#ccc",
        // progressBar
        "progressBar.background": "#42A1FF",
        // scrollbar
        "scrollbar.shadow": "#00000000",
        "scrollbarSlider.activeBackground": "#42A1FF",
        "scrollbarSlider.background": "#42A1FF80",
        "scrollbarSlider.hoverBackground": "#42A1FF",
        // selection
        "selection.background": "#3480cc",
        // sidebar
        "sideBar.background": "#131415",
        "sideBar.border": "#131415",
        "sideBar.foreground": "#ccc",
        "sideBarSectionHeader.background": "#131415",
        "sideBarSectionHeader.foreground": "#cccccc",
        "sideBarTitle.foreground": "#cccccc",
        // statusBar
        "statusBar.background": "#42a1ff",
        "statusBar.border": "#131415",
        "statusBar.debuggingBackground": "#000",
        "statusBar.debuggingBorder": "#42a1ff",
        "statusBar.debuggingForeground": "#42a1ff",
        "statusBar.foreground": "#131415",
        "statusBar.noFolderBackground": "#000",
        "statusBar.noFolderBorder": "#131415",
        "statusBar.noFolderForeground": "#fff",
        "statusBarItem.activeBackground": "#42a1ff",
        "statusBarItem.hoverBackground": "#13141520",
        "statusBarItem.prominentBackground": "#131415",
        "statusBarItem.prominentHoverBackground": "#13141520",
        // tab
        "tab.activeBackground": "#42a1ff10",
        "tab.activeForeground": "#fff",
        "tab.border": "#131415",
        "tab.activeBorder": "#42A1FF",
        "tab.inactiveBackground": "#131415",
        "tab.inactiveForeground": "#ccc",
        "tab.unfocusedActiveForeground": "#ccc",
        "tab.unfocusedInactiveForeground": "#ccc",
        // --- workbench: terminal
        "terminal.ansiBlack": "#131415",
        "terminal.ansiRed": "#cb2d25",
        "terminal.ansiGreen": "#4ad962",
        "terminal.ansiYellow": "#42A1FF",
        "terminal.ansiBlue": "#0088ff",
        "terminal.ansiMagenta": "#fb94ff",
        "terminal.ansiCyan": "#80fcff",
        "terminal.ansiWhite": "#ffffff",
        "terminal.ansiBrightBlack": "#0050A4",
        "terminal.ansiBrightRed": "#cb2d25",
        "terminal.ansiBrightGreen": "#4ad962",
        "terminal.ansiBrightYellow": "#42A1FF",
        "terminal.ansiBrightBlue": "#0088ff",
        "terminal.ansiBrightMagenta": "#fb94ff",
        "terminal.ansiBrightCyan": "#80fcff",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.background": "#131415",
        "terminal.foreground": "#ffffff",
        "terminalCursor.background": "#42A1FF",
        "terminalCursor.foreground": "#42A1FF",
        // Git status colors in File Explorer
        "gitDecoration.modifiedResourceForeground": "#42A1FF",
        "gitDecoration.deletedResourceForeground": "#cb2d25",
        "gitDecoration.untrackedResourceForeground": "#4ad962",
        "gitDecoration.ignoredResourceForeground": "#808080",
        "gitDecoration.conflictingResourceForeground": "#ffb300",
        // textBlockQuote
        "textBlockQuote.background": "#193549",
        "textBlockQuote.border": "#0088ff",
        "textCodeBlock.background": "#193549",
        "textLink.activeForeground": "#0088ff",
        "textLink.foreground": "#0088ff",
        "textPreformat.foreground": "#42A1FF",
        "textSeparator.foreground": "#131415",
        "titleBar.activeBackground": "#131415",
        "titleBar.activeForeground": "#ccc",
        "titleBar.inactiveBackground": "#131415",
        "titleBar.inactiveForeground": "#ffffff33",
        "walkThrough.embeddedEditorBackground": "#131415",
        "welcomePage.buttonBackground": "#193549",
        "welcomePage.buttonHoverBackground": "#131415",
        "widget.shadow": "#00000026"
    },
    // "tokenColors": "./Sto Theme.tmTheme"
    "tokenColors": [
        {
            "name": "Comment",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#888"
            }
        },
        {
            "name": "Constant",
            "scope": "constant",
            "settings": {
                "foreground": "#FF68B8"
            }
        },
        {
            "name": "Entity",
            "scope": "entity",
            "settings": {
                "foreground": "#91c2ff"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "#cb2d25"
            }
        },
        {
            "name": "Storage Type Function",
            "scope": "storage.type.function",
            "settings": {
                "foreground": "#91c2ff"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword, storage.type.class, keyword.control.default.ts",
            "settings": {
                "foreground": "#91c2ff"
            }
        },
        {
            "name": "Meta",
            "scope": "meta",
            "settings": {
                "foreground": "#ccc"
            }
        },
        {
            "name": "Meta JSX",
            "scope": ["meta.jsx.children", "meta.jsx.children.js", "meta.jsx.children.tsx"],
            "settings": {
                "foreground": "#fff"
            }
        },
        {
            "name": "Meta Brace",
            "scope": "meta.brace",
            "settings": {
                "foreground": "#e1efff"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#ccc"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "Storage Type Arrow Function",
            "scope": "storage.type.function.arrow",
            "settings": {
                "foreground": "#91c2ff"
            }
        },
        {
            "name": "String",
            "scope": ["string", "punctuation.definition.string"],
            "settings": {
                "foreground": "#dcdcff"
            }
        },
        {
            "name": "String Template",
            "scope": ["string.template", "punctuation.definition.string.template"],
            "settings": {
                "foreground": "#dcdcff"
            }
        },
        {
            "name": "Support",
            "scope": "support",
            "settings": {
                "foreground": "#dcdcff"
            }
        },
        {
            "name": "Support Function",
            "scope": "support.function",
            "settings": {
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "Support Variable Property DOM",
            "scope": "support.variable.property.dom",
            "settings": {
                "foreground": "#e1efff"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "foreground": "#e1efff"
            }
        },
        {
            "name": "[CSS] - Entity",
            "scope": ["source.css entity", "source.stylus entity"],
            "settings": {
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "[CSS] - ID Selector",
            "scope": "entity.other.attribute-name.id.css",
            "settings": {
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "[CSS] - Element Selector",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#FF68B8"
            }
        },
        {
            "name": "[CSS] - Support",
            "scope": ["source.css support", "source.stylus support"],
            "settings": {
                "foreground": "#dcdcff"
            }
        },
        {
            "name": "[CSS] - Constant",
            "scope": ["source.css constant", "source.css support.constant", "source.stylus constant", "source.stylus support.constant"],
            "settings": {
                "foreground": "#FF68B8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "[CSS] - String",
            "scope": ["source.css string", "source.css punctuation.definition.string", "source.stylus string", "source.stylus punctuation.definition.string"],
            "settings": {
                "foreground": "#FF68B8"
            }
        },
        {
            "name": "[CSS] - Variable",
            "scope": ["source.css variable", "source.stylus variable"],
            "settings": {
                "foreground": "#FF68B8"
            }
        },
        {
            "name": "[HTML] - Entity Name",
            "scope": "text.html.basic entity.name",
            "settings": {
                "foreground": "#FF68B8"
            }
        },
        {
            "name": "[HTML] - ID value",
            "scope": "meta.toc-list.id.html",
            "settings": {
                "foreground": "#A5FF90"
            }
        },
        {
            "name": "[HTML] - Entity Other",
            "scope": "text.html.basic entity.other",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "[HTML] - Script Tag",
            "scope": "meta.tag.metadata.script.html entity.name.tag.html",
            "settings": {
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "[HTML] - Quotes. these are a slightly different colour because expand selection will then not include quotes",
            "scope": "punctuation.definition.string.begin, punctuation.definition.string.end",
            "settings": {
                "foreground": "#ccc"
            }
        },
        {
            "name": "[INI] - Entity",
            "scope": "source.ini entity",
            "settings": {
                "foreground": "#e1efff"
            }
        },
        {
            "name": "[INI] - Keyword",
            "scope": "source.ini keyword",
            "settings": {
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "[JAVASCRIPT] - Storage Type Function",
            "scope": "source.js storage.type.function, source.ts storage.type.function",
            "settings": {
                "foreground": "#fb94ff"
            }
        },
        {
            "name": "[JAVASCRIPT] - Variable Language",
            "scope": "variable.language, entity.name.type.class.js",
            "settings": {
                "foreground": "#fb94ff"
            }
        },
        {
            "name": "[JAVASCRIPT] - Inherited Component",
            "scope": "entity.other.inherited-class.js",
            "settings": {
                "foreground": "#ccc"
            }
        },
        {
            "name": "[PYTHON] - Self Argument",
            "scope": "variable.parameter.function.language.special.self.python",
            "settings": {
                "foreground": "#fb94ff"
            }
        },
        {
            "name": "[JSON] - Support",
            "scope": "source.json support",
            "settings": {
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "[JSON] - String",
            "scope": ["source.json string", "source.json punctuation.definition.string"],
            "settings": {
                "foreground": "#e1efff"
            }
        },
        {
            "name": "[MARKDOWN] - Heading Punctuation",
            "scope": "punctuation.definition.heading.markdown",
            "settings": {
                "foreground": "#e1efff"
            }
        },
        {
            "name": "[MARKDOWN] - Heading Name Section",
            "scope": ["entity.name.section.markdown", "markup.heading.setext.1.markdown", "markup.heading.setext.2.markdown"],
            "settings": {
                "foreground": "#42A1FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "[MARKDOWN] - Paragraph",
            "scope": "meta.paragraph.markdown",
            "settings": {
                "foreground": "#e1efff"
            }
        },
        {
            "name": "[MARKDOWN] - Quote Punctuation",
            "scope": "beginning.punctuation.definition.quote.markdown",
            "settings": {
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "[MARKDOWN] - Quote Paragraph",
            "scope": "markup.quote.markdown meta.paragraph.markdown",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ccc"
            }
        },
        {
            "name": "[MARKDOWN] - Separator",
            "scope": "meta.separator.markdown",
            "settings": {
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "[MARKDOWN] - Emphasis Bold",
            "scope": "markup.bold.markdown",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ccc"
            }
        },
        {
            "name": "[MARKDOWN] - Emphasis Italic",
            "scope": "markup.italic.markdown",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ccc"
            }
        },
        {
            "name": "[MARKDOWN] - Lists",
            "scope": "beginning.punctuation.definition.list.markdown",
            "settings": {
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "[MARKDOWN] - Link Title",
            "scope": "string.other.link.title.markdown",
            "settings": {
                "foreground": "#FF68B8"
            }
        },
        {
            "name": "[MARKDOWN] - Link/Image Title",
            "scope": ["string.other.link.title.markdown", "string.other.link.description.markdown", "string.other.link.description.title.markdown"],
            "settings": {
                "foreground": "#FF68B8"
            }
        },
        {
            "name": "[MARKDOWN] - Link Address",
            "scope": ["markup.underline.link.markdown", "markup.underline.link.image.markdown"],
            "settings": {
                "foreground": "#ccc"
            }
        },
        {
            "name": "[MARKDOWN] - Inline Code",
            "scope": ["fenced_code.block.language", "markup.inline.raw.markdown"],
            "settings": {
                "foreground": "#ccc"
            }
        },
        {
            "name": "[MARKDOWN] - Code Block",
            "scope": ["fenced_code.block.language", "markup.inline.raw.markdown"],
            "settings": {
                "foreground": "#ccc"
            }
        },
        {
            "name": "[PHP] - Entity",
            "scope": "source.php entity",
            "settings": {
                "foreground": "#ccc"
            }
        },
        {
            "name": "[PHP] - Variables",
            "scope": "variable.other.php",
            "settings": {
                "foreground": "#42A1FF"
            }
        },
        {
            "name": "Italicsify for Operator Mono",
            "scope": [
                "modifier",
                "this",
                "comment",
                "storage.modifier",
                "entity.other.attribute-name.js",
                // "storage.modifier.js",
                // "storage.modifier.ts",
                // "storage.modifier.tsx",
                "entity.other.attribute-name.js",
                "entity.other.attribute-name.ts",
                "entity.other.attribute-name.tsx",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Export Default",
            "scope": ["keyword.control.export"],
            "settings": {
                "foreground": "#FF68B8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "[TYPESCRIPT] Returned Type",
            "scope": ["meta.return.type.ts"],
            "settings": {
                "foreground": "#FF68B8",
                "fontStyle": "italic"
            }
        }
    ]
}
